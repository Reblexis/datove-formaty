@prefix ex: <https://example.org/vocabulary/> .
@prefix data: <https://example.org/data/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

######################### CLASSES #########################

ex:Person a rdfs:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "A human individual with basic personal information."@en .

ex:Teacher a rdfs:Class ;
    rdfs:label "Teacher"@en ;
    rdfs:subClassOf ex:Person ;
    rdfs:comment "A person who teaches one or more subjects."@en .

ex:Student a rdfs:Class ;
    rdfs:label "Student"@en ;
    rdfs:subClassOf ex:Person ;
    rdfs:comment "A person who attends one or more subjects."@en .

ex:Subject a rdfs:Class ;
    rdfs:label "Subject"@en ;
    rdfs:comment "A taught subject including its code and name."@en .

ex:Building a rdfs:Class ;
    rdfs:label "Building"@en ;
    rdfs:comment "A building that can contain classrooms and where teachers or students may reside."@en .

ex:Classroom a rdfs:Class ;
    rdfs:label "Classroom"@en ;
    rdfs:comment "A classroom located in a building where subjects take place."@en .

ex:Specialization a rdfs:Class ;
    rdfs:label "Specialization"@en ;
    rdfs:comment "A specialization is studied by students."@en .


######################### DATA PROPERTIES #########################

ex:firstName a rdf:Property ;
    rdfs:label "First name"@en ;
    rdfs:domain ex:Person ;
    rdfs:range rdf:langString .

ex:lastName a rdf:Property ;
    rdfs:label "Last name"@en ;
    rdfs:domain ex:Person ;
    rdfs:range rdf:langString .

ex:mobileNumber a rdf:Property ;
    rdfs:label "Mobile number"@en ;
    rdfs:domain ex:Person ;
    rdfs:range rdfs:Resource .

ex:salary a rdf:Property ;
    rdfs:label "Salary"@en ;
    rdfs:domain ex:Teacher ;
    rdfs:range xsd:integer .

ex:name a rdf:Property ;
    rdfs:label "Name"@en ;
    rdfs:domain ex:Subject ;
    rdfs:range rdf:langString .

ex:code a rdf:Property ;
    rdfs:label "Subject code"@en ;
    rdfs:domain ex:Subject ;
    rdfs:range xsd:string .

ex:address a rdf:Property ;
    rdfs:label "Address"@en ;
    rdfs:domain ex:Building ;
    rdfs:range xsd:string .

ex:buildingName a rdf:Property ;
    rdfs:label "Building name"@en ;
    rdfs:domain ex:Building ;
    rdfs:range rdf:langString .

ex:capacity a rdf:Property ;
    rdfs:label "Classroom capacity"@en ;
    rdfs:domain ex:Classroom ;
    rdfs:range xsd:integer .

ex:classroomName a rdf:Property ;
    rdfs:label "Classroom name"@en ;
    rdfs:domain ex:Classroom ;
    rdfs:range rdf:langString .

ex:title a rdf:Property ;
    rdfs:label "Specialization title"@en ;
    rdfs:domain ex:Specialization ;
    rdfs:range rdf:langString .

ex:numberOfStudents a rdf:Property ;
    rdfs:label "Number of students"@en ;
    rdfs:domain ex:Specialization ;
    rdfs:range xsd:integer .


######################### OBJECT PROPERTIES #########################

# Person -> Building
ex:resides a rdf:Property ;
    rdfs:label "resides in"@en ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Building ;
    rdfs:comment "Links a person to the building where they reside."@en .

# Teacher -> Subject
ex:teaches a rdf:Property ;
    rdfs:label "teaches"@en ;
    rdfs:domain ex:Teacher ;
    rdfs:range ex:Subject ;
    rdfs:comment "Links a teacher to a subject they teach."@en .

# Student -> Specialization
ex:studies a rdf:Property ;
    rdfs:label "studies"@en ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Specialization ;
    rdfs:comment "Links a student to the specialization they study."@en .

# Subject -> Classroom
ex:takes_place a rdf:Property ;
    rdfs:label "takes place in"@en ;
    rdfs:domain ex:Subject ;
    rdfs:range ex:Classroom ;
    rdfs:comment "Links a subject to the classroom where it takes place."@en .

# Student -> Subject
ex:attends a rdf:Property ;
    rdfs:label "attends"@en ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Subject ;
    rdfs:comment "Links a student to a subject they attend."@en .

# Classroom -> Building
ex:is_part_of a rdf:Property ;
    rdfs:label "is part of"@en ;
    rdfs:domain ex:Classroom ;
    rdfs:range ex:Building ;
    rdfs:comment "Links a classroom to the building it belongs to."@en .

